function check1(){
//var level=localStorage.getItem("level");//alert(level);
    if(JSON.parse( localStorage.getItem(JSONObject))[random].answer==1){
      if(parseInt(localStorage.getItem("attempt"))==0){
       // score=parseInt(localStorage.getItem("score"));alert(score);
        //score=score+ (100*mult(level));
        var temp=parseInt(localStorage.getItem("score"))+ (100*mult(level));
        localStorage.setItem("score",temp);
        localStorage.setItem("attempt",0);
  }else if(parseInt(localStorage.getItem("attempt"))==1){
        //score=parseInt(localStorage.getItem("score"));alert(score);
        //parseInt(localStorage.getItem("score"))+ (60*mult(level));
        var temp=parseInt(localStorage.getItem("score"))+ (60*mult(level));
        localStorage.setItem("score",temp);
        localStorage.setItem("attempt",0);
  }else if(parseInt(localStorage.getItem("attempt"))==2){
        //score=parseInt(localStorage.getItem("score"));alert(score);
       // score=score+ (30*mult(level));
        var temp=parseInt(localStorage.getItem("score"))+ (30*mult(level));
        localStorage.setItem("score",temp);
        localStorage.setItem("attempt",0);
  }
  else if(parseInt(localStorage.getItem("attempt"))==3){
        //score=parseInt(localStorage.getItem("score"));alert(score);
        //score=score+ (10*mult(level));
      var temp=parseInt(localStorage.getItem("score"))+ (10*mult(level));
        localStorage.setItem("score",temp);
        localStorage.setItem("attempt",0);
  }location.reload();}
    else {attempt=parseInt(localStorage.getItem("attempt")); 
    attempt=attempt+1; if(attempt>3)attempt=3; 
    localStorage.setItem("attempt",attempt);}
  }


